package pair.concrete;

import concretesyntax.ConcreteJava;

import pair.concrete.Syntax;

public sugar Desugar {
  desugarings
    pair2expr
    pair2type

  rules
    pair2expr : 
      |[ (~expr:e1, ~expr:e2) ]| 
      ->
      |[ pair.Pair.create(~e1, ~e2) ]|

    pair2type :
      |[ (~type:t1, ~type:t2) ]|
      ->
      |[ pair.Pair<~t1, ~t2> ]| 
}