MODULE Foo

IMPORT org.sugarj.languages.Fomega

context-free syntax
  "FUN" FomegaTID "::" FomegaKind "." FomegaType -> FomegaType {cons("FunTyAbs")}

rules
  desugar-fun-type-abs : 
    FunTyAbs(name,kind,type) 
    -> TyAbs(name,kind,type)

desugarings
  desugar-fun-type-abs

rules
  constraint-warning :
    t@FunTyAbs(name, kind, type) -> (t, "\"FUN\" keyword is depreciated.")
