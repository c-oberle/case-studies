MODULE type.language.FomegaRelation

IMPORT org.sugarj.languages.Fomega
IMPORT type.language.RelationDeclaration

context-free syntax
  "()" -> Ctx {cons("CtxEmpty")}
  Ctx ";" FomegaVID ":" FomegaType -> Ctx {cons("CtxBindVar")}
  Ctx ";" FomegaTID "::" FomegaKind -> Ctx {cons("CtxBindTVar")}

type relation kcheck
  Ctx |- FomegaType :: FomegaKind

type relation tcheck
  Ctx |- FomegaExpr : FomegaType
