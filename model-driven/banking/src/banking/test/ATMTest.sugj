package banking.test;

import statemachine.ToJava(banking.atm.ATM);

public class ATMTest { 
  public static void main(String[] args) {
    ATM machine = new ATM(); 
    machine.step(machine.event_InsertCard());
    machine.step(machine.event_PinOK());
    machine.step(machine.event_MoneyTaken());
    machine.step(machine.event_CardTaken());
    
    if (machine.currentState() == machine.state_Init())
      System.out.println("success");
    else
      System.out.println("failure");
  }
}
