package entity;

public class TestUtil {
  public static void testEqual(Object expected, Object actual) {
  	boolean equal;
  	if (expected == null && actual == null)
  	  equal = true;
  	else if (expected != null && expected.equals(actual))
  	  equal = true;
  	else
  	  equal = false;
  	
  	if (equal)
  	  System.out.println("Test ok: " + actual + " is " + expected);
  	else
  	  System.out.println("Test not ok: " +actual + " should have been " + expected);
  }

  public static void testNotEqual(Object expected, Object actual) {
  	boolean equal;
  	if (expected == null && actual == null)
  	  equal = true;
  	else if (expected != null && expected.equals(actual))
  	  equal = true;
  	else
  	  equal = false;
  	
  	if (!equal)
  	  System.out.println("Test ok: " + actual + " is not " + expected);
  	else
  	  System.out.println("Test not ok: " +actual + " should not have been " + expected);
  }
}