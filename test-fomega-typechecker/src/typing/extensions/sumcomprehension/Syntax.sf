module typing.extensions.sumcomprehension.Syntax

import org.sugarj.languages.Fomega

context-free syntax
  "for" FomegaID "=" FomegaExpr "to" FomegaExpr "add" FomegaExpr -> FomegaExpr {cons("SumComp")}

context-free priorities
  FomegaExpr FomegaExpr -> FomegaExpr >
  "for" FomegaID "=" FomegaExpr "to" FomegaExpr "add" FomegaExpr -> FomegaExpr

lexical syntax
  "for" -> Keyword
  "to"  -> Keyword
  "add" -> Keyword

lexical restrictions
  "for" "to" "add" -/- [A-Za-z0-9]
