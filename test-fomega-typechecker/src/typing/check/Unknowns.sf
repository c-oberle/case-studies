module typing.check.Unknowns

import org.sugarj.languages.Fomega

signature
  constructors
    TyUnknown: FomegaType
    IDUnknown: FomegaID

rules
  typeOf = // debug(!"TYPEOF IN:  "); 
           typeOf1
           // ; debug(!"TYPEOF OUT: ")

  typeOf1 = typeOfKnown <+ typeOfUnknown

  typeOfKnown = fail

  typeOfUnknown :
    (ctx, e) -> (TyUnknown(), [(e, "Missing typing rule")])
