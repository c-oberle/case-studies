package entity;

import entity.TestUtil;
import entity.AccountRecord;

public class TestAccountRecord {
  public static void main(String[] args) {
     AccountRecord acc1 = new AccountRecord(1, 30000, "xxxx");
     AccountRecord acc2 = acc1.setBalance(20000);
     TestUtil.testEqual(30000, acc1.getBalance());
     TestUtil.testEqual(20000, acc2.getBalance());
  }
}
