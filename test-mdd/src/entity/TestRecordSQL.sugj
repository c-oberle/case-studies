package entity;

import AccountRecord = entity.EntityToRecord(entity.Account);
import AccountSQL = entity.EntityToSQL(entity.Account);

public class TestRecordSQL {
  public static void main(String[] args) {
     AccountRecord acc1 = new AccountRecord(1, 30000, "xxxx");
     AccountSQL acc2 = new AccountSQL(1, 30000, "xxxx");
     acc1.setBalance(20000);
     acc2.setBalance(20000);
     testNotEqual(acc1.getBalance(), acc2.getBalance());
  }


  public static void testNotEqual(Object expected, Object actual) {
  	boolean equal;
  	if (expected == null && actual == null)
  	  equal = true;
  	else if (expected != null && expected.equals(actual))
  	  equal = true;
  	else
  	  equal = false;
  	
  	if (!equal)
  	  System.out.println("Test ok: " + actual + " is not " + expected);
  	else
  	  System.out.println("Test not ok: " +actual + " should not have been " + expected);
  }
}